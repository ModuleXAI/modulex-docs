{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "quill",
  "name": "ModuleX AI",
  "colors": {
    "primary": "#1A1A1A",
    "light": "#F5F5F5",
    "dark": "#0D0D0D",
    "background": {
      "light": "#FFFFFF",
      "dark": "#1A1A1A"
    }
  },
  "favicon": "/favicon.svg",
  "font": {
    "headings": {
      "family": "Inter",
      "weight": 500
    },
    "body": {
      "family": "Inter"
    }
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": false
  },
  "rounded": "default",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://modulex.ai/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/ModuleXAI",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@modulex.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.modulex.ai"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/modulexai",
      "github": "https://github.com/ModuleXAI",
      "linkedin": "https://linkedin.com/company/modulexai"
    }
  },
  "codeBlock": {
    "mode": "auto"
  },
  "appearance": {
    "sidebar": {
      "style": "default"
    }
  }
}
